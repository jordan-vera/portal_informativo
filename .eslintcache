r\n                                <div className=\"col-lg-5\">\r\n                                    <label>Email</label>\r\n                                    <input onChange={this.handleChange} className=\"form-control\" type=\"email\" name=\"email\" value={this.state.form.email} />\r\n                                </div>\r\n                            </div>\r\n\r\n                            <label className=\"lbl\">Formación académica</label>\r\n                            <textarea onChange={this.handleChange} className=\"form-control\" name=\"formacion_academica\" value={this.state.form.formacion_academica} required ></textarea>\r\n\r\n                            <label className=\"lbl\">Experiencia laboral</label>\r\n                            <textarea onChange={this.handleChange} className=\"form-control\" name=\"experiencia_laboral\" value={this.state.form.experiencia_laboral}></textarea>\r\n\r\n                            <div className=\"cont-btn\">\r\n                                <button className=\"btn btn-secondary\">\r\n                                    <i className=\"far fa-save\"></i> Guardar docente\r\n                                </button>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                </form>\r\n            </React.Fragment>\r\n        );\r\n    }\r\n}\r\n\r\nexport default EditDocente;",{"ruleId":"175","replacedBy":"176"},{"ruleId":"177","replacedBy":"178"},{"ruleId":"179","severity":1,"message":"180","line":42,"column":45,"nodeType":"181","endLine":42,"endColumn":87},{"ruleId":"179","severity":1,"message":"180","line":45,"column":45,"nodeType":"181","endLine":45,"endColumn":86},{"ruleId":"182","severity":1,"message":"183","line":54,"column":72,"nodeType":"184","messageId":"185","endLine":54,"endColumn":74},{"ruleId":"179","severity":1,"message":"180","line":59,"column":53,"nodeType":"181","endLine":59,"endColumn":143},{"ruleId":"179","severity":1,"message"